names_list = ["annu", "shivam", "deepa", "pooja", "rupa", "dhruv", "alok"]
if "shivam" in names_list:
    print("Shivam ka naam names_list mei hai")
else:
    print("Shivam ka naam names_list mei nahi hai.")
